{
  "currency": {
    "title": "Currency",
    "type": "string",
    "enum": [
      "CHF",
      "CAD",
      "EUR",
      "GBP",
      "USD",
      "AUD"
    ],
    "default": "CHF",
    "pattern": "^[A-Z]{3}$",
    "widget": {
      "formlyConfig": {
        "type": "select",
        "props": {
          "itemCssClass": "w-full md:w-2/12",
          "sort": true
        }
      }
    }
  }
}
