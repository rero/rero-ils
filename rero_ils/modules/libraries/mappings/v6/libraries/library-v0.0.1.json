          "type": "keyword"
        },
        "code": {
          "type": "keyword"
        },
        "name": {
          "type": "text"
        },
        "address": {
          "type": "text"
        },
        "email": {
          "type": "keyword"
        },
        "_created": {
          "type": "date"
        },
        "_updated": {
          "type": "date"
        }
      }
    }
  }
}