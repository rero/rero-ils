              {
                "key": "authors[].type",
                "type": "select",
                "condition": {
                  "functionBody": "{var author = model.authors[arrayIndices[0]]; return author.$ref === undefined && author.name === undefined && author.date === undefined &&  author.qualifier === undefined;}"
